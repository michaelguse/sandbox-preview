<script>
    $("div#wrapperGrid").Grid({

        search: true,
        selecting: true,
        pagination: {
            enabled: true,
            limit: 12
        },
        sort: true,

        columns: [{
            name: "Instance",
            //formatter: (cell) => html(`<b>${cell}</b>`),
            width: '25%'
        },{
            name: "Type",
            width: '25%'
        },{
            name: "Region",
            width: '25%'
        },{
            name: "Release",
            width: '25%'
        }],

        data: [
            <% results.forEach(function(r) { %>
            {
                instance: '<%= r.Instance %>', 
                url: 'https://sf-sandbox-preview.herokuapp.com/sandbox/<%= r.Instance %>', 
                type: '<%= r.Type %>', 
                region: '<%= r.Region %>', 
                release: "<%- r.Release %>"
            },
            <% }); %> 
        ]
    });
</script>